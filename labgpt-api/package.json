{
    "name": "medical-lab-interpreter",
    "version": "1.0.0",
    "description": "API to interpret medical lab results using OpenAI",
    "main": "server.js",
    "scripts": {
        "build": "npx puppeteer browsers install chrome",
        "postinstall": "npx puppeteer browsers install chrome --platform=linux --force",
        "start": "node server.js",
        "dev": "nodemon server.js"
    },
    "dependencies": {
        "body-parser": "^1.20.3",
        "cors": "^2.8.5",
        "crypto": "^1.0.1",
        "dotenv": "^16.3.1",
        "express": "^4.21.2",
        "marked": "^15.0.12",
        "mongoose": "^8.15.1",
        "morgan": "^1.10.0",
        "nodemailer": "^7.0.3",
        "openai": "^4.0.0",
        "pdfkit": "^0.17.1",
        "playwright": "^1.52.0",
        "puppeteer": "^24.11.1"
    },
    "devDependencies": {
        "nodemon": "^3.0.1"
    },
    "nodemonConfig": {
        "ignore": [
            "node_modules/",
            "test-client.html"
        ],
        "ext": "js,json,env"
    }
}
